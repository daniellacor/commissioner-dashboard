<!-- WRAPPER -->
<div class="wrapper">

  <!-- HOME -->
  <section class="module module-header bg-dark bg-dark-50" data-background="http://images.123hdwallpapers.com/20150517/sports-wallpaper-american-football-1024x768.jpg">

    <div class="container">

      <!-- MODULE TITLE -->
      <div class="row">

        <div class="col-sm-6 col-sm-offset-3">

          <h1 class="module-title font-alt align-center"> EDIT ACCOUNT PAGE </h1>
          <!-- QUOTE OF THE DAY

          <div class="module-subtitle font-inc align-center">
            QUOTE OF THE DAY
          </div> -->

        </div>

      </div>
      <!-- /MODULE TITLE -->

    </div>

  </section >
  <!-- /HOME -->

  <!-- PORTFOLIO -->
  <section class="module-small p-t-20 p-b-60">

    <div class="container">

      <!-- PORTFOLIO FILTER -->
      <div class="row">
        <div class="col-sm-12">

        </div>
      </div>
      <!-- /PORTFOLIO FILTER -->

    </div>

    <div class="module-title font-alt align-center">
      <%= form_for (@commissioner) do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %><br>
        <%= f.label :password %>
        <%= f.password_field :password %><br>
        <%= f.label :password_confirmation %>
        <%= f.password_field :password_confirmation %><br>
        <%= f.label :email %>
        <%= f.text_field :email %><br>
        <%= f.label :phone_number %>
        <%= f.text_field :phone_number %><br>
        <%= f.submit 'Update', class: "btn btn-g btn-circle btn-lg"  %>
      <% end %>
    <div><br>

  </section>
  <!-- /PORTFOLIO -->

  <!-- DIVIDER -->
  <hr class="divider-w">
  <!-- /DIVIDER -->


  <!-- FOOTER -->
  <footer class="footer">

    <div class="container">

      <div class="row">

        <div class="col-sm-12 text-center">
