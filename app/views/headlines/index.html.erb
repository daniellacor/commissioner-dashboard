<div class="wrapper">

  <!-- HOME -->
  <%= render partial: "leagues/dashboard_jumbotron", locals: {league: @league} %>
   <!-- /HOME -->
   <!-- PORTFOLIO DESCRIPTION -->
   <%= render "layouts/messages" %>
   <section class="module-small">

     <div class="container">

       <div class="row">

         <div class="col-sm-6">

           <div class="work-details">
             <h3 class="work-details-title font-alt">NFL NEWS</h3>
              <p><strong>Today's Bleacher Report</strong></p>
              <% @headlines.each do |title, link| %>
                <%= link_to title, link, :target => '_blank' %><br>
              <% end %>
           </div>
           <%= link_to 'Email to League', headlines_path, class: "btn btn-d btn-circle btn-lg" %>
         </div>

         <div class="col-sm-6">


       </div>

     </div>

   </section>
